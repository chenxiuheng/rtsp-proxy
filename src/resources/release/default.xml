<assembly>
	<id>default</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>target</directory>
			<includes>
				<include>rtspproxy*.jar</include>
			</includes>
			<outputDirectory>/lib</outputDirectory>
		</fileSet>
		<fileSet>
			<includes>
				<include>*.txt</include>
			</includes>
			<outputDirectory>/</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/resources/scripts</directory>
			<includes>
				<include>*</include>
			</includes>
			<fileMode>755</fileMode>
			<outputDirectory>/</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/resources/conf</directory>
			<includes>
				<include>*</include>
			</includes>
			<outputDirectory>/conf</outputDirectory>
		</fileSet>
	</fileSets>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/lib</outputDirectory>
			<unpack>false</unpack>
			<scope>runtime</scope>
		</dependencySet>
	</dependencySets>
</assembly>
